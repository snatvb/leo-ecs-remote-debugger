{
  "name": "leo-ecs-remote-debugger",
  "version": "2020.3.22-preview",
  "description": "Electron debugger for leo ecs",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.snatvb.leo-ecs-remote-debugger",
    "productName": "Leo Ecs remote debugger",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "2020.3.22-preview"
  },
  "author": "Andrew Brigthon @snatvb",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@types/events": "3.0.0",
    "@types/ramda": "0.26.44",
    "@types/react": "16.9.13",
    "@types/react-custom-scrollbars": "4.0.6",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "5.0.1",
    "@types/ws": "7.2.2",
    "bufferutil": "4.0.1",
    "events": "3.1.0",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "monad-maniac": "^0.4.0",
    "polished": "3.4.4",
    "ramda": "0.27.0",
    "react": "16.12.0",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.12.0",
    "styled-components": "5.0.1",
    "ws": "7.2.3"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/react-router": "4.4.5",
    "@types/react-router-dom": "4.3.5",
    "acorn": "6.4.0",
    "ajv": "6.10.2",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "5.2.1",
    "css-loader": "1.0.1",
    "electron": "3.1.13",
    "electron-builder": "20.44.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "lodash": "4.17.15",
    "mocha": "5.2.0",
    "rimraf": "2.7.1",
    "source-map-loader": "0.2.4",
    "spectron": "5.0.0",
    "style-loader": "0.23.1",
    "ts-loader": "6.2.1",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript": "3.8.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snatvb/leo-ecs-remote-debugger"
  },
  "keywords": [
    "ecs",
    "leo",
    "remote",
    "debugger",
    "leopotam"
  ],
  "bugs": {
    "url": "https://github.com/snatvb/leo-ecs-remote-debugger/issues"
  },
  "homepage": "https://github.com/snatvb/leo-ecs-remote-debugger#readme"
}
